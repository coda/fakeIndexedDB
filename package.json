  },
  "devDependencies": {
    "browserify": "^10.1.3",
    "eslint": "^7.7.0",
    "mocha": "^2.2.4",
    "node-qunit-phantomjs": "^1.2.0",
    "phantomjs-polyfill": "0.0.1",
    "qunitjs": "^1.18.0",
    "setimmediate": "^1.0.2"
  }
}
